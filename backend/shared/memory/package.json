{
  "name": "@fineprintai/shared-memory",
  "version": "1.0.0",
  "description": "Production-ready shared memory service for Fine Print AI autonomous agents",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist/**/*"],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:performance": "jest --testPathPattern=performance",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "start": "node dist/index.js",
    "start:dev": "tsx src/index.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@aws-sdk/client-s3": "^3.470.0",
    "@aws-sdk/s3-request-presigner": "^3.470.0",
    "fastify": "^4.25.2",
    "@fastify/websocket": "^8.3.1",
    "@fastify/cors": "^8.4.2",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.1.0",
    "ioredis": "^5.3.2",
    "zod": "^3.22.4",
    "bcryptjs": "^2.4.3",
    "crypto": "^1.0.1",
    "uuid": "^9.0.1",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "dotenv": "^16.3.1",
    "node-cron": "^3.0.3",
    "bullmq": "^5.1.9",
    "prometheus-client": "^15.1.0",
    "vectordb": "^0.4.0",
    "compromise": "^14.10.0",
    "natural": "^6.10.4",
    "ml-matrix": "^6.10.7",
    "date-fns": "^3.0.6"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.4",
    "@types/uuid": "^9.0.7",
    "@types/node-cron": "^3.0.11",
    "@types/natural": "^5.1.4",
    "jest": "^29.7.0",
    "prisma": "^5.7.1",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "supertest": "^6.3.3",
    "@types/supertest": "^6.0.2"
  },
  "peerDependencies": {
    "@fineprintai/shared-types": "workspace:*",
    "@fineprintai/shared-utils": "workspace:*",
    "@fineprintai/shared-security": "workspace:*",
    "@fineprintai/shared-config": "workspace:*"
  }
}