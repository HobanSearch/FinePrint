{
  "name": "@fineprintai/content-marketing-agent",
  "version": "1.0.0",
  "description": "Autonomous content marketing system for Fine Print AI",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "dependencies": {
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^8.0.3",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^2.1.0",
    "@fineprintai/shared-config": "*",
    "@fineprintai/shared-types": "*",
    "@fineprintai/shared-utils": "*",
    "fastify": "^4.25.2",
    "zod": "^3.22.4",
    "@prisma/client": "^5.7.1",
    "redis": "^4.6.10",
    "ioredis": "^5.3.2",
    "bullmq": "^4.15.1",
    "openai": "^4.24.1",
    "ollama": "^0.4.4",
    "cheerio": "^1.0.0-rc.12",
    "axios": "^1.6.2",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "@sendgrid/mail": "^7.7.0",
    "@google-analytics/data": "^4.0.0",
    "keyword-extractor": "^0.0.25",
    "natural": "^6.6.0",
    "sentiment": "^5.0.2",
    "mammoth": "^1.6.0",
    "pdf-parse": "^1.1.1",
    "sharp": "^0.32.6",
    "canvas": "^2.11.2",
    "puppeteer": "^21.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/jest": "^29.5.8",
    "@types/lodash": "^4.14.202",
    "@types/uuid": "^9.0.7",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/index.ts"
    ]
  },
  "keywords": [
    "content-marketing",
    "ai",
    "automation",
    "seo",
    "social-media",
    "email-marketing",
    "analytics",
    "legal-tech"
  ],
  "author": "Fine Print AI Team",
  "license": "MIT"
}