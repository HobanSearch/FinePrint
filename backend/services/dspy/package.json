{
  "name": "@fineprintai/dspy-service",
  "version": "1.0.0",
  "description": "DSPy-powered prompt optimization service for Fine Print AI",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "setup:python": "cd python && pip install -r requirements.txt",
    "dev:python": "cd python && python -m uvicorn main:app --reload --port 8007",
    "dev:full": "concurrently \"npm run dev:python\" \"npm run dev\""
  },
  "dependencies": {
    "fastify": "^4.25.2",
    "fastify-plugin": "^4.5.1",
    "@fastify/cors": "^8.4.2",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.0.1",
    "@fastify/websocket": "^10.0.1",
    "axios": "^1.6.2",
    "zod": "^3.22.4",
    "bullmq": "^5.0.0",
    "ioredis": "^5.3.2",
    "ws": "^8.16.0",
    "uuid": "^9.0.1",
    "concurrently": "^8.2.2",
    "@fineprintai/shared-config": "workspace:*",
    "@fineprintai/shared-logger": "workspace:*",
    "@fineprintai/shared-memory": "workspace:*",
    "@fineprintai/auth": "workspace:*",
    "@fineprintai/queue": "workspace:*",
    "@fineprintai/shared-cache": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/ws": "^8.5.10",
    "@types/uuid": "^9.0.7",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0",
    "@vitest/coverage-v8": "^1.1.0",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0"
  },
  "keywords": [
    "dspy",
    "llm",
    "prompt-optimization",
    "legal-analysis",
    "business-optimization",
    "ai-optimization",
    "fastify",
    "microservice",
    "machine-learning",
    "prompt-engineering"
  ],
  "author": "Fine Print AI Team",
  "license": "MIT"
}