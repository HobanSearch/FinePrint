{
  "name": "@fineprint/improvement-orchestrator",
  "version": "1.0.0",
  "description": "Automated model improvement orchestration service with Temporal workflows",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext ts",
    "temporal:dev": "docker-compose -f temporal/docker-compose.yml up -d",
    "temporal:stop": "docker-compose -f temporal/docker-compose.yml down",
    "worker": "tsx src/worker.ts"
  },
  "dependencies": {
    "@temporalio/client": "^1.9.0",
    "@temporalio/worker": "^1.9.0",
    "@temporalio/workflow": "^1.9.0",
    "@temporalio/activity": "^1.9.0",
    "fastify": "^4.25.0",
    "@fastify/cors": "^8.5.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/websocket": "^8.3.1",
    "pino": "^8.17.0",
    "pino-pretty": "^10.3.0",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "@prisma/client": "^5.7.0",
    "bullmq": "^5.1.0",
    "axios": "^1.6.0",
    "zod": "^3.22.4",
    "date-fns": "^3.0.0",
    "lodash": "^4.17.21",
    "mathjs": "^12.2.0",
    "simple-statistics": "^7.8.3",
    "ml-matrix": "^6.10.4",
    "natural": "^6.10.0",
    "@tensorflow/tfjs-node": "^4.15.0",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/lodash": "^4.14.202",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.3.0",
    "tsx": "^4.6.0",
    "vitest": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "prisma": "^5.7.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}