{
  "name": "@fine-print-ai/privacy-scoring",
  "version": "1.0.0",
  "description": "Privacy scoring and document analysis service for Fine Print AI",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "analyze:top50": "tsx src/scripts/analyze-top50.ts",
    "export:training-data": "tsx src/scripts/export-training-data.ts",
    "migrate": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev"
  },
  "dependencies": {
    "@fastify/cors": "^8.5.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.1.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.12.1",
    "@fastify/swagger-ui": "^2.0.1",
    "@fastify/websocket": "^8.3.1",
    "@prisma/client": "^5.7.0",
    "@qdrant/js-client-rest": "^1.7.0",
    "axios": "^1.6.2",
    "bullmq": "^5.0.0",
    "cheerio": "^1.0.0-rc.12",
    "fastify": "^4.25.0",
    "ioredis": "^5.3.2",
    "natural": "^6.10.0",
    "neo4j-driver": "^5.15.0",
    "ollama": "^0.4.1",
    "pdfjs-dist": "^4.0.189",
    "pino": "^8.17.0",
    "puppeteer": "^21.6.1",
    "readability": "^0.4.4",
    "sharp": "^0.33.1",
    "textract": "^2.5.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/natural": "^5.1.4",
    "@types/node": "^20.10.0",
    "@types/puppeteer": "^7.0.4",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "prisma": "^5.7.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}