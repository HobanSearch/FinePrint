{
  "name": "@fineprint/learning-pipeline",
  "version": "1.0.0",
  "description": "Continuous learning pipeline for Fine Print AI model management",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "validate": "npm run lint && npm run test",
    "migrate": "prisma migrate dev",
    "generate": "prisma generate",
    "train": "tsx src/training/orchestrator.ts",
    "evaluate": "tsx src/evaluation/evaluator.ts",
    "monitor": "tsx src/monitoring/dashboard.ts"
  },
  "dependencies": {
    "@fastify/autoload": "^5.8.0",
    "@fastify/cors": "^8.5.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^7.2.4",
    "@fastify/multipart": "^8.1.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/websocket": "^8.3.1",
    "@prisma/client": "^5.7.0",
    "@tensorflow/tfjs-node": "^4.17.0",
    "axios": "^1.6.5",
    "bullmq": "^5.1.0",
    "dvc": "^0.0.4",
    "fastify": "^4.25.2",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "mlflow": "^2.9.2",
    "node-cron": "^3.0.3",
    "optuna": "^0.1.0",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "prometheus-query": "^3.4.0",
    "sharp": "^0.33.2",
    "simple-statistics": "^7.8.3",
    "tensorboard": "^0.1.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@vitest/coverage-v8": "^1.2.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.2",
    "prisma": "^5.7.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}