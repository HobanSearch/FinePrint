{
  "name": "@fineprintai/websocket-service",
  "version": "1.0.0",
  "description": "Real-time WebSocket service with Socket.io and Redis clustering",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "docker:build": "docker build -t fineprintai/websocket-service .",
    "docker:run": "docker run -p 8080:8080 fineprintai/websocket-service"
  },
  "dependencies": {
    "@fineprintai/shared-types": "*",
    "@fineprintai/shared-config": "*",
    "@fineprintai/shared-logger": "*",
    "@fineprintai/shared-cache": "*",
    "@fineprintai/shared-middleware": "*",
    "@fineprintai/shared-security": "*",
    "fastify": "^4.25.2",
    "@fastify/cors": "^8.5.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.13.0",
    "@fastify/swagger-ui": "^2.1.0",
    "socket.io": "^4.7.4",
    "@socket.io/redis-adapter": "^8.2.1",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.22.4",
    "bull": "^4.12.2",
    "uuid": "^9.0.1",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.3.3",
    "tsx": "^4.6.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "eslint": "^8.56.0",
    "@socket.io/admin-ui": "^0.5.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}