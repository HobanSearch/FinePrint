{
  "name": "@fineprint/ab-testing",
  "version": "1.0.0",
  "description": "A/B Testing Framework for Fine Print AI Model Management",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:performance": "k6 run tests/performance/load-test.js",
    "lint": "eslint . --ext .ts",
    "migrate": "prisma migrate dev",
    "generate": "prisma generate",
    "seed": "tsx scripts/seed.ts",
    "analyze": "tsx scripts/analyze-experiments.ts"
  },
  "dependencies": {
    "@fastify/cors": "^8.5.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.12.1",
    "@fastify/swagger-ui": "^2.0.1",
    "@prisma/client": "^5.7.0",
    "fastify": "^4.25.0",
    "ioredis": "^5.3.2",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "bullmq": "^5.1.0",
    "simple-statistics": "^7.8.3",
    "jstat": "^1.9.6",
    "mathjs": "^12.2.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "dayjs": "^1.11.10",
    "lodash": "^4.17.21",
    "@types/lodash": "^4.14.202",
    "hash-sum": "^2.0.0",
    "p-queue": "^8.0.1",
    "p-retry": "^6.2.0",
    "exponential-backoff": "^3.1.1",
    "node-cron": "^3.0.3",
    "prom-client": "^15.1.0",
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/sdk-node": "^0.45.1",
    "@opentelemetry/auto-instrumentations-node": "^0.40.1",
    "@opentelemetry/exporter-prometheus": "^0.45.1",
    "murmurhash3js": "^3.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/jest": "^29.5.11",
    "@types/uuid": "^9.0.7",
    "@types/node-cron": "^3.0.11",
    "typescript": "^5.3.3",
    "tsx": "^4.6.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "prisma": "^5.7.0",
    "k6": "^0.48.0",
    "@faker-js/faker": "^8.3.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}