{
  "name": "@fineprint/qa-automation",
  "version": "1.0.0",
  "description": "Comprehensive QA Automation Framework for Fine Print AI",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsx watch src/index.ts",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "vitest run --config vitest.config.ts --coverage",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:contract": "pact-broker can-i-deploy --pacticipant qa-automation",
    "test:performance": "k6 run scripts/performance/load-test.js",
    "test:security": "npm run security:scan && npm run security:audit",
    "test:chaos": "litmus run chaos --scenario network-delay",
    "test:all": "npm run test && npm run test:e2e && npm run test:performance",
    "coverage": "vitest run --coverage",
    "coverage:report": "nyc report --reporter=html --reporter=text",
    "lint": "eslint src tests --ext .ts,.tsx",
    "lint:fix": "eslint src tests --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "security:scan": "snyk test",
    "security:audit": "npm audit --audit-level=moderate",
    "deps:check": "depcheck",
    "deps:update": "npm-check-updates -u",
    "report:generate": "tsx scripts/generate-report.ts",
    "clean": "rimraf dist coverage reports/.tmp",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "validate": "npm run lint && npm run test:unit && npm run security:audit"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.11.0",
    "@fastify/autoload": "^5.8.0",
    "@fastify/cors": "^8.5.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^7.2.4",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.13.0",
    "@grafana/k6": "^0.48.0",
    "@pact-foundation/pact": "^12.1.0",
    "@prisma/client": "^5.7.0",
    "@qdrant/qdrant-js": "^1.7.0",
    "axios": "^1.6.2",
    "bullmq": "^5.1.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.25.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "pino": "^8.17.0",
    "pino-pretty": "^10.3.0",
    "temporal": "^0.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@playwright/test": "^1.40.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitest/coverage-v8": "^1.1.0",
    "@vitest/ui": "^1.1.0",
    "artillery": "^2.0.0",
    "artillery-plugin-expect": "^2.0.0",
    "autocannon": "^7.14.0",
    "axe-playwright": "^2.0.0",
    "chai": "^4.3.10",
    "chance": "^1.1.11",
    "cheerio": "^1.0.0-rc.12",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.7",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.0",
    "eslint-plugin-security": "^2.1.0",
    "eslint-plugin-sonarjs": "^0.23.0",
    "expect": "^29.7.0",
    "factory.ts": "^1.4.1",
    "fishery": "^2.2.2",
    "husky": "^8.0.3",
    "jest-html-reporters": "^3.1.5",
    "jest-junit": "^16.0.0",
    "k6": "^0.0.0",
    "lint-staged": "^15.2.0",
    "litmus": "^2.2.0",
    "madge": "^6.1.0",
    "msw": "^2.0.11",
    "nock": "^13.4.0",
    "npm-check-updates": "^16.14.12",
    "nyc": "^15.1.0",
    "p-limit": "^5.0.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "sinon": "^17.0.1",
    "snyk": "^1.1266.0",
    "supertest": "^6.3.3",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.0",
    "wait-on": "^7.2.0",
    "zap-cli": "^0.18.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fineprint-ai/qa-automation.git"
  },
  "keywords": [
    "qa",
    "automation",
    "testing",
    "e2e",
    "performance",
    "security",
    "ai",
    "llm"
  ],
  "author": "Fine Print AI QA Team",
  "license": "MIT",
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}