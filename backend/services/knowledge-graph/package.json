{
  "name": "@fineprintai/knowledge-graph",
  "version": "1.0.0",
  "description": "Knowledge Graph Service with Neo4j integration and Curriculum Learning",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "docker:build": "docker build -t fineprintai/knowledge-graph .",
    "docker:run": "docker run -p 3007:3007 fineprintai/knowledge-graph"
  },
  "dependencies": {
    "@fastify/cors": "^8.4.2",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^2.1.0",
    "@fineprintai/shared-config": "*",
    "@fineprintai/shared-logger": "*",
    "@fineprintai/shared-types": "*",
    "@fineprintai/shared-middleware": "*",
    "@fineprintai/shared-security": "*",
    "fastify": "^4.25.2",
    "neo4j-driver": "^5.17.0",
    "graphql": "^16.8.1",
    "mercurius": "^13.3.3",
    "ioredis": "^5.3.2",
    "axios": "^1.6.5",
    "zod": "^3.22.4",
    "pino": "^8.17.2",
    "nanoid": "^5.0.4",
    "lodash": "^4.17.21",
    "ml-matrix": "^6.10.7",
    "ml-kmeans": "^6.0.0",
    "compromise": "^14.13.0",
    "natural": "^6.8.0",
    "stemmer": "^2.0.1",
    "cosine-similarity": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}