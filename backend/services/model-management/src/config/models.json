        "rules": [
          {
            "condition": "complexity < 3 && urgency < 3",
            "preferredModels": ["fine-print-llama"]
          },
          {
            "condition": "complexity >= 3 && complexity < 7",
            "preferredModels": ["fine-print-qwen-v2", "fine-print-llama"]
          },
          {
            "condition": "complexity >= 7",
            "preferredModels": ["fine-print-qwen-v2"]
          }
        ]
      },
      "performance-optimized": {
        "description": "Minimize response time",
        "rules": [
          {
            "condition": "urgency >= 8",
            "preferredModels": ["fine-print-llama", "fine-print-customer"]
          },
          {
            "condition": "urgency >= 5 && urgency < 8",
            "preferredModels": ["fine-print-llama", "fine-print-qwen-v2"]
          }
        ]
      },
      "balanced": {
        "description": "Balance between cost and performance",
        "rules": [
